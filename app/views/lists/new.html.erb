<div class="container my-5">
<h1 class="text-center pb-3">Add a list</h1>
  <div class="text-center d-flex align-items-center justify-content-between pb-3">
    <%= link_to "Back to my lists", lists_path, class: "btn btn-secondary" %>
  </div>
<%= simple_form_for @list do |f| %>
  <%= f.input :name %>
  <%= f.input :image, as: :file, label: "Add a image" %>
  <br>
  <%= f.submit "Create List", class: "btn btn-primary" %>
<% end %>
</div>
